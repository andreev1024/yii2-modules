            "andreev1024\\csvImport\\": "csvImport/",
            "andreev1024\\currency\\": "currency/",
            "andreev1024\\mediaContentManager\\": "mediaContentManager/",
            "andreev1024\\piwik\\": "piwik/",
            "andreev1024\\rbac\\": "rbac/",
            "andreev1024\\recron\\": "recron/",
