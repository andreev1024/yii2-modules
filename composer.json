        "yiisoft/yii2": "*",
        "yiisoft/yii2-imagine": "2.0.*",
        "2amigos/yii2-chartjs-widget": "2.0.*@dev",
        "miloschuman/yii2-highcharts-widget": "dev-master",
        "kartik-v/yii2-icons": "*",
        "kartik-v/yii2-mpdf": "dev-master",
        "reseed/re-widgets": "@dev",
        "andreev1024/yii2-components": "dev-master as 1.0.x-dev"
    },
    "autoload": {
